{
    "problemMatcher": [
        {
            "owner": "brew-style",
            "pattern": [
                {
                    "regexp": "== (.+) ==",
                    "file": 1
                },
                {
                    "regexp": "[RCWEF]:\\s+(\\d+):\\s+(\\d+): (.+)$",
                    "line": 1,
                    "column": 2,
                    "message": 3,
                    "loop": true
                }
            ]
        }
    ]
} 
