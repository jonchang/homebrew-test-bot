{
    "problemMatcher": [
        {
            "owner": "brew-audit",
            "pattern": [
                {   "__comment": "`brew audit --display-filename`, regular audits",
                    "regexp": "^(.+): \\* (.+)$",
                    "file": 1,
                    "message": 2
                },
                {   "__comment": "`brew audit --display-filename`, style checks",
                    "regexp": "^(.+): \\* [RCWEF]: (\\d+): col (\\d+): (.+)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "message": 4
                },
                {   "__comment": "`brew style --display-filename` (emacs output)",
                    "regexp": "^(.+):(\\d+):(\\d+): [RCWEF]: (.+)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "message": 4
                },
                {   "__comment": "`brew style` (multiline matcher, header)",
                    "regexp": "== (.+) ==",
                    "file": 1
                },
                {   "__comment": "`brew style` (multiline matcher, body)",
                    "regexp": "[RCWEF]: (\\d+): (\\d+): (.+)$",
                    "line": 1,
                    "column": 2,
                    "message": 3,
                    "loop": true
                }
            ]
        }
    ]
} 
